    <validationRules>
        <fullName>Approval_Date_is_required</fullName>
        <active>false</active>
        <description>The approval date is required and</description>
        <errorConditionFormula>AND (OR( ISPICKVAL(Filing_Strategy__c, &apos;Group Filing&apos;),
      ISPICKVAL(Filing_Strategy__c, &apos;Compact Filing&apos;)
     ),
     ISBLANK( State_Approval_Date__c )
     )</errorConditionFormula>
        <errorDisplayField>Filing_Strategy__c</errorDisplayField>
        <errorMessage>Approval date is required for Group filing and Compact filing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Approved_Policy_State</fullName>
        <active>true</active>
        <description>Once the filing_status__c is approved you cannot edit it</description>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Filing_Status__c), &quot;Approved&quot;),
    ISCHANGED(Filing_Status__c)
)</errorConditionFormula>
        <errorDisplayField>Filing_Status__c</errorDisplayField>
        <errorMessage>Once the status is approved you cannot modify the status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Feature_Variation_Description_Required</fullName>
        <active>true</active>
        <description>Variation Description is required if Is Variation is checked.</description>
        <errorConditionFormula>AND(
$User.ByPassValidation__c == false,      
Is_Feature_Variation__c  = true,
    LEN(  Feature_Variation_Description__c ) = 0)</errorConditionFormula>
        <errorDisplayField>Feature_Variation_Description__c</errorDisplayField>
        <errorMessage>If Is Feature Variation is checked, then Feature Variation Description must be filled out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Form_Variation_Description_Required</fullName>
        <active>true</active>
        <description>Variation Description is required if Is Variation is checked.</description>
        <errorConditionFormula>AND(
    $User.ByPassValidation__c == false,
    Is_Form_Variation__c = true,
    LEN( Form_Variation_Description__c) = 0)</errorConditionFormula>
        <errorDisplayField>Form_Variation_Description__c</errorDisplayField>
        <errorMessage>If Is Form Variation is checked, then Form Variation Description must be filled out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_is_Approved_Require_Approval_Date</fullName>
        <active>false</active>
        <description>If the policy form state status is approved, require the state approval date if empty UNLESS dependent approval exists.</description>
        <errorConditionFormula>((ISPICKVAL(Filing_Status__c,&quot;Approved&quot;)) 

&amp;&amp;  (ISNULL(State_Approval_Date__c )) &amp;&amp; (NOT (ISPICKVAL(  Filing_Strategy__c , &quot;Compact Dependent&quot; )) || (ISPICKVAL(  Filing_Strategy__c , &quot;Group Dependent&quot; ))))</errorConditionFormula>
        <errorDisplayField>State_Approval_Date__c</errorDisplayField>
        <errorMessage>To change the Status to Approved for individual filing state or situs/compact filing states, enter the State Approval Date first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Withdrawn_Date_Required</fullName>
        <active>true</active>
        <description>If Withdrawn status is entered manually, require withdrawn date</description>
        <errorConditionFormula>ISPICKVAL(Filing_Status__c, &quot;Withdrawn&quot;) &amp;&amp;  ISBLANK( Filing_Withdrawn_Date__c ) &amp;&amp;  ( Number_of_Objections__c  &gt; 0)&amp;&amp; (Filing_Objection__r.Will_Not_Be_Responding__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Filing_Withdrawn_Date__c</errorDisplayField>
        <errorMessage>Please enter a Filing Withdrawn Date in order to change the filing status to &quot;Withdrawn&quot;.</errorMessage>
    </validationRules>
