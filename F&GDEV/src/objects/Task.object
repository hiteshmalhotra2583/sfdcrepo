    <validationRules>
        <fullName>Call_Outcome_Required_if_Type_is_Call</fullName>
        <active>true</active>
        <description>The call outcome is required if the type is a call.</description>
        <errorConditionFormula>((ISPICKVAL(Type, &quot;Outbound Call&quot;))|| (ISPICKVAL(Type, &quot;Inbound Call&quot;))) &amp;&amp;  (ISBLANK (TEXT(Call_Outcome__c )))</errorConditionFormula>
        <errorDisplayField>Call_Outcome__c</errorDisplayField>
        <errorMessage>Since the type of activity is Call, choose a value for Call Outcome.</errorMessage>
    </validationRules>
    <validationRules>
