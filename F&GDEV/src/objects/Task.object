        <active>false</active>
        <description>Notes must be entered (not N/A) in tasks.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; IsBlank( Notes__c )</errorConditionFormula>
        <errorDisplayField>Notes__c</errorDisplayField>
        <errorMessage>Notes must be completed and not contain solely &quot;N/A&quot;.</errorMessage>
    </validationRules>
